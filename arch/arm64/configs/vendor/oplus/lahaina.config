#
# OPlus lahaina config
#

# Crypto
CONFIG_CRYPTO_LZ4=y

# Display
CONFIG_DRM_OPLUS_NOTIFY=m
CONFIG_DRM_OPLUS_PANEL_NOTIFY=m

# exFAT
CONFIG_EXFAT_FS=y

# Fingeprint
CONFIG_OPLUS_FINGERPRINT=y
CONFIG_OPLUS_FINGERPRINT_COMMON=m
CONFIG_OPLUS_FINGERPRINT_QCOM=y
CONFIG_OPLUS_SECURE=y
CONFIG_OPLUS_SECURE_QCOM=y
CONFIG_OPLUS_SECURE_COMMON=m

# GPIO
CONFIG_OPLUS_GPIO=y

# Header tests
# CONFIG_UAPI_HEADER_TEST is not set

# OPlus Project
CONFIG_OPLUS_DEVICE_IFNO=y
CONFIG_OPLUS_FEATURE_OPROJECT=y
CONFIG_OPLUS_FEATURE_PROJECTINFO=y
CONFIG_OPLUS_FEATURE_FEEDBACK=y
CONFIG_OPLUS_FEATURE_ANDROIDBOOT=y

# Power
CONFIG_OPLUS_CALL_MODE_SUPPORT=y
CONFIG_OPLUS_CHECK_CHARGERID_VOLT=y
CONFIG_OPLUS_CHG=m
CONFIG_OPLUS_CHIP_SOC_NODE=y
CONFIG_OPLUS_SHIP_MODE_SUPPORT=y
CONFIG_OPLUS_SHORT_C_BATT_CHECK=y
CONFIG_OPLUS_SHORT_USERSPACE=y
CONFIG_OPLUS_SM8350_CHARGER=y
CONFIG_OPLUS_SMART_CHARGER_SUPPORT=y

# Sim Detect
CONFIG_SIM_DETECT=y

# Touchscreen
CONFIG_TOUCHPANEL_OPLUS=m
CONFIG_TOUCHIRQ_UPDATE_QOS=y
CONFIG_TOUCHPANEL_CUSTOM=m
CONFIG_TOUCHPANEL_NOTIFY=m

# UFS
# CONFIG_FS_HPB is not set
# CONFIG_SCSI_SKHPB is not set
# CONFIG_UFSHPB is not set
CONFIG_OPLUS_FEATURE_UFS_DRIVER=y
CONFIG_OPLUS_FEATURE_PADL_STATISTICS=y
CONFIG_UFSFEATURE=y
CONFIG_UFSHID=y
CONFIG_UFSHID_POC=y
CONFIG_UFSTW=y
CONFIG_UFSTW_BOOT_ENABLED=y
CONFIG_UFSTW_IGNORE_GUARANTEE_BIT=y

# WiFi
CONFIG_OPLUS_FEATURE_WIFI_BDF=y
CONFIG_OPLUS_FEATURE_WIFI_BDF2=y
CONFIG_OPLUS_FEATURE_WIFI_MAC=y
CONFIG_QCA_CLD_WLAN=m
